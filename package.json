{
  "name": "deli",
  "version": "0.0.0",
  "private": true,
  "description": "deli",
  "license": "OGL",
  "scripts": {
    "clean": "rimraf built",
    "compile": "tsc --outDir built",
    "webpack": "npm run clean && webpack --progress",
    "build": "npm run clean && npm run compile && webpack",
    "build:prod": "set NODE_ENV=production && npm run build",
    "dev": "concurrently -k \"npm run compile && nodemon built/app.server/server.js\" \"webpack-dev-server --inline --hot \"",
    "dev:server": "npm run compile && nodemon built/app.server/server.js",
    "test": "karma start --single-run",
    "test:watch": "karma start",
    "start": "node built/app.server/server.js"
  },
  "dependencies": {
    "body-parser": "1.16.1",
    "dotenv": "2.0.0",
    "exports-loader": "0.6.3",
    "express": "4.14.0",
    "imports-loader": "0.6.5",
    "leaflet": "1.0.1",
    "lodash": "4.17.4",
    "moment": "2.17.0",
    "nprogress": "0.2.0",
    "query-string": "4.2.3",
    "random-key": "0.3.2",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-flip-move": "2.7.0-beta1",
    "react-modal": "1.7.1",
    "turf": "3.0.14",
    "url": "0.11.0",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "0.0.33",
    "@types/express": "4.0.33",
    "@types/leaflet": "1.0.36",
    "@types/lodash": "4.14.54",
    "@types/moment": "2.13.0",
    "@types/nprogress": "0.0.29",
    "@types/react": "15.0.14",
    "@types/react-dom": "0.14.18",
    "@types/react-modal": "1.6.2",
    "concurrently": "3.1.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.1",
    "csv-parser": "1.11.0",
    "extract-text-webpack-plugin": "2.0.0-rc.3",
    "file-loader": "0.10.0",
    "html-webpack-plugin": "2.28.0",
    "less": "2.7.2",
    "less-loader": "2.2.3",
    "nodemon": "1.11.0",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.4",
