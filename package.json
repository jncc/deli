{
  "name": "deli",
  "private": true,
  "scripts": {
    "clean": "rimraf built",
    "compile": "tsc --outDir built && cp ./app.server/data/*.json ./built/app.server/data",
    "webpack": "npm run clean && webpack --progress",
    "build": "npm run clean && npm run compile && webpack",
    "build:prod": "npm run clean && npm run compile && webpack -p --env.name production",
    "dev": "npm run compile && concurrently -k \"nodemon built/app.server/server.js\" \"webpack-dev-server --inline --hot \"",
    "dev:server": "npm run compile && nodemon built/app.server/server.js",
    "test": "karma start --single-run",
    "test:watch": "karma start",
    "start": "node built/app.server/server.js"
  },
